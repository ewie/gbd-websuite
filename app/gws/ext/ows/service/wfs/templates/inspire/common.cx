@def inspire_id(node, theme)
    @t gml:identifier codeSpace="http://inspire.jrc.ec.europa.eu/ids" {node.attributes.catalogUid.value}

    @tag {theme}:inspireId/base:Identifier
        @t base:localId {node.attributes.localUid.value}
        @t base:namespace {node.attributes.namespace.value}
    @end
@end
