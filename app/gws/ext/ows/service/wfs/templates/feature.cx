@include /gws-app/gws/common/ows/service/templates/common.cx

@tag wfs:member
    @tag {feature_ns(node.tag_name)} gml:id="{node.feature.uid}"
        @tag {feature_ns('geometry')}
            @insert {node.shape_tag}
        @end
        @each node.attributes.values() as a
            @t {feature_ns(a.name)} {a.value}
        @end
    @end
@end
